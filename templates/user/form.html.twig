{{ form_start(form) }}

  <div class="form-row">
    <div class="col-4">
      {{ form_row(form.username) }}
    </div>
  </div>

  <div class="form-row">
    <div class="col-4">
      {{ form_row(form.email) }}
    </div>
  </div>

  <div class="form-row">
    <div class="col-4">
      {{ form_row(form.plainPassword) }}
    </div>
  </div>

  <button class="btn btn-primary" type="submit">Save</button>

{{ form_end(form) }}